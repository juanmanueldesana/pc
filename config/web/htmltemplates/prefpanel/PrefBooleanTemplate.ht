<div id="${name}" class="gw-Pref gw-ValueWidget gw-editable" data-gw-change="gwPrefPanel.change" data-gw-getset="checkbox">
    <div class="gw-Pref--inner">
        <div class="gw-label">${label}</div>
        <div class="gw-value">
            <input class="gw-Pref--val gw-Pref--boolean" name="${name}" type="checkbox" checked="${checked}">
        </div>
    </div>
</div>
