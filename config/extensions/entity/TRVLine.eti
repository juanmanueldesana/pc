    desc="Line-level modifiers for Seguro de Viaje"
    arrayentity="TRVLineMod"
    owner="false"
    cascadeDelete="true"/>

  <array
    name="TRVCovers"
    desc="Traveler Insured"
    arrayentity="TRVCover"
    owner="true"
    cascadeDelete="true"/>

  <array
    name="TRVCoverCosts"
    desc="Traveler Costs"
    arrayentity="TRVCoverCost"
    owner="false"
    cascadeDelete="true"/>

</subtype>