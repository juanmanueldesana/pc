  supertype="PolicyLine">
  <implementsInterface
    iface="gw.api.policy.PolicyLineMethods"
    impl="gw.lob.trv.TRVPolicyLineMethods"/>
  <implementsEntity
    name="Coverable"/>
  <implementsInterface
    iface="gw.api.domain.CoverableAdapter"
    impl="gw.lob.trv.TRVLineCoverableModifiableAdapter"/>
  <implementsEntity
    name="Modifiable"/>
